{
  "name": "spryker-shop/order-custom-reference-widget",
  "type": "library",
  "description": "OrderCustomReferenceWidget module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/kernel": "^3.33.0",
    "spryker/order-custom-reference": "^1.0.0",
    "spryker/quote": "^2.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker-shop/shop-ui": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/router": "*"
  },
  "suggest": {
    "spryker/router": "Use this module when using plugins that need Router dependencies."
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
